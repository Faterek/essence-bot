{
  "name": "essence-bot-backend",
  "version": "0.0.1",
  "private": true,
  "license": "CC-BY-SA-4.0",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "bun build ./src/main.ts --outdir ./build",
    "build:changelog": "bunx @discordx/changelog --src src",
    "dev": "bun run ./src/main.ts",
    "start": "bun run ./build/main.js",
    "watch": "bun --hot run ./src/main.ts"
  },
  "dependencies": {
    "@discordx/importer": "1.3.0",
    "@discordx/koa": "1.2.1",
    "@discordx/pagination": "3.5.0",
    "@koa/router": "12.0.1",
    "discord.js": "14.14.1",
    "discordx": "11.9.0",
    "koa": "2.15.0"
  },
  "devDependencies": {
    "@types/koa": "2.15.0",
    "@types/koa__router": "12.0.4",
    "@types/bun": "1.0.8",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "typescript": "5.4.2"
  }
}
